    "version": "0.2.0",
    "configurations": [
        {
            "name": "test parser",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "file", 
                "--format", "string",
                "--filename","/Users/eliofrancesconi/Documents/Projects/easylog/test.log",
                "--excludes", "level,msg,service"
            ],
            "program": "${workspaceFolder}/cmd/elog"
        },
        {
            "name": "remote-tail",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "args": [
                "remote-tail", 
                "--format", "json",
                "--includes", "time,msg,file"
            ],
            "program": "${workspaceFolder}/cmd/elog"
        }
    ]
